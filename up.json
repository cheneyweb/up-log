{
  "name": "up-log",
  "hooks": {
    "build": "make"
  },
  "proxy": {
    "command": "./node-v8.9.3-linux-x64/bin/node app.js"
  },
  "lambda": {
    "memory": 128
  },
  "cors": {
    "allowed_origins": ["*"],
    "allowed_methods": ["HEAD", "GET", "POST", "PUT", "PATCH", "DELETE"],
    "allowed_headers": ["*"]
  }
}